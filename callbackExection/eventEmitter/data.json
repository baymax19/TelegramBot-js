  'testReadDirectories.js': 'const fs = require("fs")\nconst util = require("util")\n\nfunction read_directory(path, next){\n\tfs.readdir(".",function (err,files){\n\t\tlet count = files.length;\n\t\tlet result = {};\n\t\tfiles.forEach((file) => {\n\t\t\tfs.readFile(file,\'utf8\', (err,data)=>{\n\t\t\t\tif (err){console.log(err);}\n\t\t\t\telse{\n\t\t\t\tresult[file] =  parseString(data);\n\t\t\t\tcount -- ;\n//\t\t\t\tconsole.log(data);\n\t\t\t\t}\n\t\t\t\tif (count <=0){next(result);}\n\t\t\t});\n\t\t})\n\t})\t\n\t\n}\nread_directory("eventEmitter",function(data){\n\tfs.writeFileSync("data.json", util.inspect(data) ,"utf8",(err) =>{\n\t\tconsole.log("file saved");\n\t})\n\t});\n\nfunction parseString(str) {\n    return str.replace(/\\r/g, \'\\r\').replace(/\\n/g, \'\\n\').replace(/\\t/g, \'\\t\')\n}\n',
